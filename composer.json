    "license": "MIT",
    "authors": [
        {
            "name": "Acit Jazz",
            "email": "acitjazz@gmail.com",
            "homepage": "https://eurekapage.com"
        }
    ],
    "homepage": "https://github.com/acitjazz/uploadendpoint",
    "keywords": ["Laravel", "UploadEndpoint"],
    "require": {
        "illuminate/support": "~9",
        "league/glide-laravel": "^1.0",
        "cloudinary-labs/cloudinary-laravel": "^2.0",
        "league/flysystem-aws-s3-v3": "^3.10"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
